package tavernacon;

import java.util.ArrayList;
import java.util.Collection;
/**
 *
 * @author Vijay
 */
public class Workflows {

    private Collection<WorkflowResource> workflows;

	public Workflows() {
		this.workflows = new ArrayList<WorkflowResource>();
	}

	public Workflows(Collection<WorkflowResource> workflows) {
		this.workflows = workflows;
	}

	public Collection<WorkflowResource> getWorkflow() {
		return workflows;
	}

	public void setWorkflow(Collection<WorkflowResource> workflows) {
		this.workflows = workflows;
	}


}
