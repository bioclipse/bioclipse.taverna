package tavernacon;


import java.io.Serializable;

/**
 * Interface for objects that are identified by a serializable ID.
 *
 * @author Vijay
 * @param <Id> the type of the identifier
 */
public interface Identifiable<Id extends Serializable> extends Dated {

	/**
	 * Returns the identifier.
	 *
	 * @return the identifier
	 */
	public Id getId();

	/**
	 * Sets the identifier.
	 *
	 * @param id the ner identifier
	 */
	public void setId(Id id);

}